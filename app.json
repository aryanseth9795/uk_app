{
  "expo": {
    "name": "UR Shop",
    "slug": "urshop",
    "scheme": "urshop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6F56BF"
    },
    "updates": {
      "enabled": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#6F56BF",
        "monochromeImage": "./assets/icon.png"
      },
      "permissions": [],
      "package": "com.aryanseth9795.urshop"
    },
    "extra": {
      "eas": {
        "projectId": "b70345c5-868e-4dce-901a-fd05810ec451"
      }
    },
    "owner": "aryanseth9795",
    "plugins": [
      "expo-secure-store"
    ]
  }
}
